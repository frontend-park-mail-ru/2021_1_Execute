    "eslint": "eslint '**/*.js'",
    "template": "find . -name \"*.handlebars\" -exec handlebars {} -f {}.js \\;",
    "start": "npm run build && node server/server.js",
    "dev": "webpack --config webpack.dev.js && webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js"
  },
